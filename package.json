{
  "name": "infomir-store",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "concurrently \"npm run client\" \"npm run server\"",
    "dev-debug": "concurrently \"npm run client\" \"npm run server-debug\"",
    "client": "npm start --prefix client",
    "server": "nodemon bin/www",
    "server-debug": "node $NODE_DEBUG_OPTION bin/www"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "~1.18.2",
    "concurrently": "^3.6.0",
    "connect-mongo": "^2.0.0",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "express-session": "^1.15.6",
    "formidable": "^1.2.1",
    "hbs": "~4.0.1",
    "mongoose": "^4.13.7",
    "mongoose-gridfs": "^0.5.0",
    "morgan": "~1.9.0",
    "nodemon": "^1.18.1",
    "redux": "^4.0.0",
    "serve-favicon": "~2.4.5"
  }
}
